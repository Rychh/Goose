// iteracyjnie
def fact (n) {
  i = 1 ;
  r = 1 ;
  while (i < n+1) {
    r = r * i ;
    i = i + 1 ;
  }
  return r ;
  printStr("Cos poszlo nie tak");

}

def factr (n) {
  if (n < 2) 
    return 1 ;
  else 
    return (n * factr(n-1)) ;
  printStr("Cos poszlo nie tak");
}

def main () {

  printInt(fact(7)) ;
  printInt(factr(7)) ;
  return 0 ;
}